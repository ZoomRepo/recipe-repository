[
     {
        "name": "DR Vegan Blog",
        "url": "https://drveganblog.com/",
        "type": "cooking",
        "recipes": {
            "listing": [
                {
                    "url": "https://drveganblog.com/category/recipes/",
                    "link_selector": "h2.entry-title a",
                    "pagination_selector": "a.next"
                }
            ]
        },
        "article": {
            "title": [
                "h1.entry-title"
            ],
            "description": [
                "div.entry-content > p"
            ],
            "ingredients": [
                "div.wprm-recipe-ingredients ul li",
                "div.wp-block-wprm-recipe-ingredients ul li"
            ],
            "instructions": [
                "div.wprm-recipe-instructions div.wprm-recipe-instruction-text",
                "div.wp-block-wprm-recipe-instructions li"
            ],
            "prep_time": [
                "span.wprm-recipe-prep_time"
            ],
            "cook_time": [
                "span.wprm-recipe-cook_time"
            ],
            "total_time": [
                "span.wprm-recipe-total_time"
            ],
            "servings": [
                "span.wprm-recipe-servings"
            ],
            "image": [
                "meta[property='og:image']",
                "figure.wp-block-image img"
            ],
            "author": [
                "span.wprm-recipe-author",
                "span.author"
            ],
            "categories": [
                "span.cat-links a"
            ],
            "tags": [
                "span.tags-links a"
            ]
        },
        "structured_data": {
            "enabled": true,
            "json_ld_selector": "script[type='application/ld+json']",
            "json_ld_path": "$..[?(@['@type']=='Recipe')]"
        }
    },
