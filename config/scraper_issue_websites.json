[
       
    {
        "name": "BBC Good Food",
        "url": "https://www.bbcgoodfood.com/",
        "type": "cooking",
        "recipes": {
            "listing": [
                {
                    "url": "https://www.bbcgoodfood.com/recipes",
                    "link_selector": "a.standard-card-new__article-title",
                    "pagination_selector": "a.pagination-item--next"
                }
            ]
        },
        "article": {
            "title": [
                "h1.post-header__title"
            ],
            "description": [
                "div.editor-content > p"
            ],
            "ingredients": [
                "section.recipe__ingredients ul.ingredients-list__content li"
            ],
            "instructions": [
                "section.method__list li.method__item"
            ],
            "prep_time": [
                "span.recipe-details__cooking-time-prep"
            ],
            "cook_time": [
                "span.recipe-details__cooking-time-cook"
            ],
            "total_time": [
                "span.recipe-details__total-time"
            ],
            "servings": [
                "span.recipe-details__item--servings"
            ],
            "image": [
                "figure img.post-header__recipe-image"
            ],
            "author": [
                "a.post-header__author-link"
            ],
            "categories": [
                "ul.tags-list li a"
            ],
            "tags": [
                "ul.tags-list li a"
            ]
        },
        "structured_data": {
            "enabled": true,
            "json_ld_selector": "script[type='application/ld+json']",
            "json_ld_path": "$..['@graph'][?(@['@type']=='Recipe')]"
        }
    },
